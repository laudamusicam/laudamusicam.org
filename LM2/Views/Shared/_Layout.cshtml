<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@("Lauda Musicam - " + ViewBag.Title)</title>
    <link href='http://fonts.googleapis.com/css?family=Diplomata|Chivo:900|Coda:800|Amarante|Alegreya:700italic|Source+Sans+Pro:900italic' rel='stylesheet' type='text/css'>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-2.5.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/_my_utils.js")" type="text/javascript"></script>
</head>
<body>
    <div class="page">
        <header>
            <div id="title" class="left">
                <a href="@Url.Action("Index", "Home")"><img alt="Lauda Musicam of Atlanta" src="@Url.Content("~/Content/LmTitle.png")" width="600px" /></a>
            </div>
            <nav class="right">
                <ul>
                    <li style="display:none"><a href="@Url.Content("~/calendar")">Lauda Calendar</a></li>
                    <li><a href="@Url.Content("~/media")">Recordings and Photos</a></li>
                    <li><a href="http://www.fippleflute.com/WEBPROTECT-laudamemberdownloads.htm">Lauda Members</a></li>
                    <li><a href="@Url.Content("~/about")">About Us</a></li>
                    <li><a href="@Url.Content("~/links")">Links</a></li>
                 </ul>
            </nav>
            <div id="photoStripContainer" class="left">
                <img id="photoStrip" alt="Photo Strip" src="@Url.Content("~/Content/PhotoStrip3.png")" />
            </div>
        </header>
        <section id="main" class="clear">
            @RenderBody()
        </section>
        <footer>©2012-2015 Lauda Musicam | <a href="mailto:lauda.musicam@gmail.com">Contact Us</a></footer>
    </div>
</body>
</html>
